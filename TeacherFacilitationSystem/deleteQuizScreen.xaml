<Window x:Class="TeacherFacilitationSystem.deleteQuizScreen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="deleteQuizScreen" Height="300" Width="300">
    <Grid>
        <Label Content="Enter quiz id" HorizontalAlignment="Left" Margin="10,44,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="delVal" HorizontalAlignment="Left" Height="23" Margin="115,44,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="158"/>
        <Button Content="Delete" HorizontalAlignment="Left" Margin="198,81,0,0" VerticalAlignment="Top" Width="75" Click="delQuiz"/>

    </Grid>
</Window>
